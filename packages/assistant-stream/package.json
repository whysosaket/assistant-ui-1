      "types": "./dist/ai-sdk.d.ts",
      "default": "./dist/ai-sdk.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    }
  },
  "source": "./src/index.ts",
  "main": "./dist/index.js",
